<view><view class="status" style="{{'position:'+(headerPosition)+';'}}"></view><view class="header" style="{{'position:'+(headerPosition)+';'}}"><view class="addr"><view class="icon location"></view>{{city}}</view><view class="input-box"><input placeholder="默认关键字" placeholder-style="color:#c0c0c0;" data-event-opts="{{[['tap',[['toSearch']]]]}}" bindtap="__e"/><view class="icon search"></view></view><view class="icon-btn"><view data-event-opts="{{[['tap',[['toMsg',['$event']]]]]}}" class="icon tongzhi" bindtap="__e"></view></view></view><view class="place"></view><view class="category-list"><scroll-view class="left" scroll-y="true"><block wx:for="{{categoryParent}}" wx:for-item="category" wx:for-index="index" wx:key="id"><view data-event-opts="{{[['tap',[['showCategory',['$0'],[[['categoryParent','id',category.id,'id']]]]]]]}}" class="{{['row',category.id==showCategoryIndex?'on':'']}}" bindtap="__e"><view class="text"><view class="block"></view>{{''+category.name+''}}</view></view></block></scroll-view><scroll-view class="right" scroll-y="true"><view class="category"><view class="banner"><image src="{{currentCategory.picUrl}}"></image></view><view class="list"><block wx:for="{{categoryList.currentSubCategory}}" wx:for-item="box" wx:for-index="i" wx:key="i"><view data-event-opts="{{[['tap',[['toCategory',['$0'],[[['categoryList.currentSubCategory','',i]]]]]]]}}" class="box" bindtap="__e"><image src="{{box.picUrl}}"></image><view class="text">{{box.name}}</view></view></block></view></view></scroll-view></view></view>